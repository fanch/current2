# Depends on: python-simplejson 
name=python-tweepy
version=2.2
release=1

source=(
http://pypi.python.org/packages/source/t/tweepy/tweepy-$version.tar.gz)

build() {
    cd tweepy-$version
    python2 setup.py install --prefix=/usr --root=$PKG
}



